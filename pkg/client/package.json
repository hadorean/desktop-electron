{
	"name": "wholepaper-client",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"build:demo": "vite build --config vite.demo.config.ts",
		"preview": "vite preview",
		"preview:demo": "vite preview --config vite.demo.config.ts",
		"check": "svelte-check --tsconfig ./tsconfig.app.json && tsc -p tsconfig.node.json",
		"format": "prettier --write src --end-of-line lf",
		"format:check": "prettier --check src --end-of-line lf",
		"lint": "eslint --cache . --config ../../eslint.config.mjs",
		"svelte-check": "svelte-check --tsconfig ./tsconfig.app.json"
	},
	"dependencies": {
		"socket.io-client": "^4.8.1"
	}
}
